{
    "name": "temp/media-converter",
    "type": "library",
    "description": "media converter",
    "keywords": ["media", "convert"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Wentz",
            "email": "stephan@wentz.it",
            "homepage": "http://www.wentz.it/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Temp\\MediaConverter\\": "src/"
