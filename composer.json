{
    "name": "temp/media-converter",
    "type": "library",
    "description": "media converter",
    "keywords": ["media", "convert"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Wentz",
            "email": "stephan@wentz.it",
            "homepage": "http://www.wentz.it/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Temp\\MediaConverter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Temp\\MediaConverter\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "~7.0",
        "symfony/filesystem": "^2.1|^3.0",
        "temp/media-classifier": "^1.0",
        "alchemy/binary-driver" : "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "mikey179/vfsStream": "^1.5",
        "php-ffmpeg/php-ffmpeg": " 0.5",
        "php-ffmpeg/extras": "^0.3",
        "imagine/imagine": "^0.6",
        "alchemy/phpexiftool": "^0.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
